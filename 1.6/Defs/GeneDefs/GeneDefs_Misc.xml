<?xml version="1.0" encoding="utf-8"?>
<Defs>
<GeneDef>
    <defName>Bulwark_IronWilled</defName>
    <label>iron will</label>
    <labelShortAdj>ironwilled</labelShortAdj>
    <description>Carriers of this gene have an iron will.</description>
    <iconPath>UI/Icons/Genes/IronWilled</iconPath>
    <displayCategory>Violence</displayCategory>
    <displayOrderInCategory>60</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>Nerves</def>
        <degree>2</degree>
      </li>
    </forcedTraits>
</GeneDef>

<GeneDef>
    <defName>Hollow_Bones</defName>
    <label>hollow bones</label>
    <description>Carriers of this gene have hollow bones that make them lighter and move faster but more susceptible to blunt damage.</description>
    <iconPath>UI/Icons/Genes/HollowBones</iconPath>
    <displayCategory>Healing</displayCategory>
    <damageFactors>
			<Blunt>1.5</Blunt>
		</damageFactors>
     <statFactors>
      <MoveSpeed>1.5</MoveSpeed>
    </statFactors>
    <biostatMet>2</biostatMet>
    <biostatCpx>1</biostatCpx>
</GeneDef>

</Defs>